{
  "name": "fibonacci-microservice",
  "version": "0.0.1",
  "description": "A simple Node app built on Express using OpenTelemetry",
  "main": "src/index.js",
  "scripts": {
    "start": "node -r ./src/tracing.js ./src/index.js",
    "metrics": ". ./.env && HNY_API_KEY=$HONEYCOMB_API_KEY HNY_DATASET=otel-nodejs-host-metrics ./otelcol_hny_linux_amd64 --config honeycomb-metrics-config.yaml"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.6.7",
    "@opentelemetry/api": "^1.2.0",
    "@opentelemetry/core": "^1.4.0",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.32.0",
    "@opentelemetry/instrumentation": "^0.32.0",
    "@opentelemetry/instrumentation-express": "^0.31.0",
    "@opentelemetry/instrumentation-http": "^0.32.0",
    "@opentelemetry/resources": "^1.6.0",
    "@opentelemetry/sdk-trace-base": "^1.6.0",
    "@opentelemetry/sdk-trace-node": "^1.6.0",
    "@opentelemetry/semantic-conventions": "^1.6.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.3"
  },
  "engines": {
    "node": "14.x"
  },
  "license": "MIT",
  "keywords": [
    "node",
    "glitch",
    "opentelemetry",
    "express"
  ]
}
